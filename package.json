        "nodes": [
            "dist/nodes/TelegramMiniAppsAuth/TelegramMiniAppsAuth.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/jest": "^29.5.14",
        "@types/node": "^24.6.2",
        "audit-ci": "^7.0.1",
        "eslint": "9.32.0",
