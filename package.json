{
    "name": "n8n-nodes-tg-miniapps-auth",
    "version": "0.1.0",
    "description": "n8n node for Telegram Mini Apps authentication and user management",
    "license": "MIT",
    "homepage": "https://github.com/dj-kostya/n8n-nodes-tg-miniapps-auth#readme",
    "keywords": [
        "n8n-community-node-package",
        "telegram",
        "mini-apps",
        "authentication",
        "telegram-bot",
        "telegram-web-app"
    ],
    "author": {
        "name": "dj-kostya",
        "email": "djkoo2001@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dj-kostya/n8n-nodes-tg-miniapps-auth.git"
    },
    "bugs": {
        "url": "https://github.com/dj-kostya/n8n-nodes-tg-miniapps-auth/issues"
    },
    "engines": {
        "node": ">=18.17.0"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --ci --coverage --watchAll=false",
        "audit": "npm audit",
        "audit:fix": "npm audit fix",
        "outdated": "npm outdated",
        "update": "npm update"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/TelegramApi.credentials.js"
